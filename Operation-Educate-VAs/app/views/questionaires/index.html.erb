<!DOCTYPE html>
<html lang="en" dir="ltr">
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        <%= form_for @questionaire do |f| %>

        <div class="field">
            <%=f.label :fname %>
            <%=f.text_field :fname %>
        </div>

        <div class="field">
            <%=f.label :lname %>
            <%=f.text_field :lname %>
        </div>

        <div class="field">
            <%=f.label :date_of_birth %>
            <%=f.text_field :date_of_birth %>
        </div>
        <div class="field">
            <%= f.label :sex %>
            <%=f.text_field :sex %>
        </div>
        <div class="field">
            <%= f.label :time_in_military %>
            <%=f.text_field :time_in_military %>
        </div>
        <div class="field">
            <%= f.label :time_in_combat %>
            <%=f.text_field :time_in_combat %>
        </div>
        <div class="field">
            <%= f.label :dependents %>
            <%=f.text_field :dependents %>
        </div>
        <div class="field">
            <%= f.label :education_level %>
            <%=f.text_field :education_level %>
        </div>
        <div class="field">
            <%= f.label :discharge_date %>
            <%=f.text_field :discharge_date %>
        </div>
        <div class="field">
            <%= f.label :part_online_fulltime %>
            <%=f.text_field :part_online_fulltime %>
        </div>
        <div class="field">
            <%= f.label :cumm_nine_elev %>
            <%=f.text_field :cumm_nine_elev %>
        </div>
            <%=f.submit "Submit"%>
        <%end%>

    </body>
</html>
